@model MasterLayoutEx.Models.EMPDATA
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_MasterLayoutPage1.cshtml";
}
    <script src="~/Scripts/MyScript.js"></script>
<h2>Index</h2>
@using (Html.BeginForm("Update","BindingEx"))
{
    <table>
        <tr>
            <td>Empno</td>
            <td>@Html.TextBoxFor(A => A.EMPNO, "", new { onblur = "Func();"})</td>
        </tr>
        <tr>
            <td>Ename</td>
            <td>@Html.TextBoxFor(A => A.ENAME)</td>
        </tr>
        <tr>
            <td>Job</td>
            <td>@Html.TextBoxFor(A => A.JOB)</td>
        </tr>
        <tr>
            <td>Mgr</td>
            <td>@Html.TextBoxFor(A => A.MGR)</td>
        </tr>
        <tr>
            <td>HireDate</td>
            <td>@Html.TextBoxFor(A => A.HIREDATE)</td>
        </tr>
        <tr>
            <td>Salary</td>
            <td>@Html.TextBoxFor(A => A.SAL)</td>
        </tr>
        <tr>
            <td>Comm</td>
            <td>@Html.TextBoxFor(A => A.COMM)</td>
        </tr>
        <tr>
            <td>Deptno</td>
            <td>@Html.TextBoxFor(A => A.DEPTNO)</td>
        </tr>
        <tr>
            <td><input type="submit" /></td>
        </tr>
    </table>
    if (ViewBag.msg != null)
    {
        <h3>@ViewBag.msg</h3>
    }
}

